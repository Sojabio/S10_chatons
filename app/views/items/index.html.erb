<header>
  <%= render partial: 'banner'%>
</header>

<main>
  <div class="product_section">
    <div class="container">
      <div class="row p-5 m-5">
        <% @items.each do |item| %>
          <div class="col-md-4 mb-4">
            <%= render partial: 'items/card', locals: { item: item } %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</main>